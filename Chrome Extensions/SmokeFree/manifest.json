{
    "manifest_version": 2,
    "name": "SmokeFree",
    "version": "1.0",
    "description": "A simple extension to track your quit date",
    "icons": {
      "48": "icon.png"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icon.png"
      }
    },
    "options_page": "options.html",
    "permissions": [
      "storage"
    ]
    
}
  